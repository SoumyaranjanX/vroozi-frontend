{
  "name": "contract-processing-system",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "tsc": "tsc -p tsconfig.app.json",
    "ng": "ng",
    "start": "ng serve --configuration=development",
    "build": "ng build",
    "build:prod": "ng build --configuration=production --optimization=true",
    "build:staging": "ng build --configuration=staging",
    "test": "ng test --code-coverage --watch=false --browsers=ChromeHeadless",
    "test:watch": "ng test",
    "test:coverage": "ng test --code-coverage --watch=false",
    "lint": "ng lint --fix",
    "e2e": "ng e2e",
    "security:audit": "npm audit && snyk test",
    "analyze:bundle": "webpack-bundle-analyzer dist/stats.json",
    "precommit": "npm run lint && npm run test"
  },
  "dependencies": {
    "@angular/animations": "^15.0.0",
    "@angular/cdk": "^15.0.0",
    "@angular/common": "^15.0.0",
    "@angular/compiler": "^15.0.0",
    "@angular/core": "^15.0.0",
    "@angular/forms": "^15.0.0",
    "@angular/material": "^15.0.0",
    "@angular/platform-browser": "^15.0.0",
    "@angular/platform-browser-dynamic": "^15.0.0",
    "@angular/router": "^15.0.0",
    "@ngrx/effects": "^15.0.0",
    "@ngrx/entity": "^15.0.0",
    "@ngrx/store": "^15.0.0",
    "@ngrx/store-devtools": "^15.0.0",
    "@ngx-translate/core": "^14.0.0",
    "@ngx-translate/http-loader": "^7.0.0",
    "@webcomponents/custom-elements": "^1.5.0",
    "crypto-js": "^4.1.1",
    "modern-normalize": "^2.0.0",
    "normalize.css": "^8.0.1",
    "rxjs": "~7.8.0",
    "zone.js": "~0.13.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^15.0.0",
    "@angular/cli": "^15.0.0",
    "@angular/compiler-cli": "^15.0.0",
    "@types/crypto-js": "^4.1.1",
    "@types/jasmine": "~4.3.0",
    "@types/jest": "^29.0.0",
    "@types/karma": "^6.3.0",
    "@types/node": "^18.11.0",
    "@typescript-eslint/eslint-plugin": "^5.47.0",
    "@typescript-eslint/parser": "^5.47.0",
    "eslint": "^8.30.0",
    "jasmine-core": "~4.5.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "^2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "karma-junit-reporter": "^2.0.1",
    "karma-parallel": "^0.3.1",
    "prettier": "^2.8.0",
    "snyk": "^1.1090.0",
    "typescript": "~4.9.0",
    "webpack": "^5.80.0",
    "webpack-bundle-analyzer": "^4.8.0"
  }
}
